[Unit]
Description=BeoSound5c MasterLink Sniffer including IR data
After=network.target
Wants=network.target

[Service]
Type=simple
User=kirsten
Group=kirsten
WorkingDirectory=/home/kirsten/beosound5c/services
EnvironmentFile=-/etc/beosound5c/config.env
ExecStart=/usr/bin/python3 /home/kirsten/beosound5c/services/masterlink.py
Restart=always
RestartSec=5
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target 
